<xml xmlns="https://developers.google.com/blockly/xml" id="toolbox" style="display: none">
  <category id="ai_camera" name="AI CAMERA" colour="#0b5394" icon="camera">
    <block type="camera_init">
      <value name="BAUDRATE">
        <shadow type="math_number">
          <field name="NUM">115200</field>
        </shadow>
      </value>
    </block>
    <block type="camera_read_data">
      <value name="END_CHAR">
        <shadow type="text">
          <field name="TEXT">\n</field>
        </shadow>
      </value>
    </block>
    <block type="camera_check_data" />
  </category>
</xml>